// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    int n, row, col, star;
    cin >> n;
    for(row = 1; row <= n; row++){
        for(col = 1; col <= n-row; col++){
            cout << " ";
        }for(star = 1; star <= row; star++){
            cout << "*";
        }
        cout << endl;
    }
    

    return 0;
}

output/-
6
     *
    **
   ***
  ****
 *****
******
