# include <stdio.h>

int main(){
int num, count;
printf("Enter the value of num\n");
scanf("%d",&num);
if (num==2||num==3){
	count=0;
}else if (num%2==0||num%3==0||num==1){
	count=1;
}else count=0;
if (count==0){
	printf("%d is a prime number",num);
}else {
	printf("%d is a not prime number");
}
return 0;
}
