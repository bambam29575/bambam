# include <stdio.h>

int main(){
int array[10],i,num,flag,no;
printf("Enter the values of num\n");
scanf("%d",&num);


for(i=0;i<num;i++){
	printf("Enter the value of element%d\n",i+1);
	scanf("%d",&array[i]);
	}
printf("Enter the value of no\n");
scanf("%d",&no);

for(i=0;i<num;i++){
	if(no==array[i]){
		flag=1;
		break;
	}else flag=0;
}
if(flag==1){
	printf("Search is successful array[%d]",i);
}
else {
	printf("Search is unsuccessful");
}
return 0;
}

Output/-
/tmp/Wl7SGhtLv1.o
