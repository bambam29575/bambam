#include <iostream>
using namespace std;

int main() {
  int num;
  cin >> num;
  char ch = 'A';
 
  
  for(int r = 1; r <= num; r++){
      
      for(int c = 1; c <= num; c++ ){
          cout << ch;
          ch++;
          
      }
      cout << endl;
      
     return 0;
    
     
     
  }
