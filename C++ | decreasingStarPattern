// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    int n, row, col, star;
    cin >> n;
    for(row = 1; row <= n; row++){
        for(col = 1; col <= n - row + 1; col++){
            cout << "*";
        }
        cout << endl;
    }
    

    return 0;
}

Output /-
5
