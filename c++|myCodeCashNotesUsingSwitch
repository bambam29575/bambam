// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
 int cash,cash100 = 100,cash50 = 50,cash20 = 20, cash1 = 1, noOfNotes, num1, num2, num3, num4;
 cin >> cash;
 
 switch(cash100){
     case 100 : num1 = 1330 % 100;
                noOfNotes = 1330 / 100;
        cout << noOfNotes<<"->"<< "100 rsCash"<< endl;
     case 50 : num2 = num1 % 50;
                noOfNotes = num1 / 50;
        cout << noOfNotes<<"->" << "50 rsCash"<< endl;
     case 20 : num3 = num2 % 20;
                noOfNotes = num2 / 20;
        cout << noOfNotes<<"->" << "20 rsCash"<< endl;
     case 1 : num4 = num3 % 1;
                noOfNotes = num3 / 1;
        cout << noOfNotes<<"->" << "1 rsCash"<< endl;
 }
    return 0;
}

output/-
1330
13->100 rsCash
0->50 rsCash
1->20 rsCash
10->1 rsCash
