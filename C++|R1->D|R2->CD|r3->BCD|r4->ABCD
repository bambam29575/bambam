#include <iostream>
using namespace std;

int main() {
    int num;
    cin >> num;
    char ch = 'D';
    for (int r = 1; r <= num; r++){
        for(int c = 1; c <= r; c++){
            
            cout << ch;
            ch++;
        }
        cout << endl;
        ch = ch - 1 - r;
    }
    

    return 0;
}
