// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    int n, i, count = 0;
    cin >> n;
    
    
   
   for(i = 2; i < n; i++){
       if (n % i == 0){
           count = 0;
           break;
       }
       else{
           count = 1;
       }
   }
   if (count == 0){
       cout << "Non Prime";
   }else {
       cout << "Prime";
   }
       
      return 0;
}


OR

// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    int n, i;
    cin >> n;
    bool isPrime = 0;
    
    for(i = 2; i < n; i++){
       if (n % i == 0 ){
           isPrime = 0;
           break;
       }
       else{
           isPrime = 1;
       }
   }
   if (isPrime == 0){
       cout << "Non Prime";
   }else {
       cout << "Prime";
   }
       
   return 0;
}
