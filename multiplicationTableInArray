#include <stdio.h>

int main() {
    int num,multiplicationTable[10];
    printf("Enter the value of num\n");
    scanf("%d",&num);
    for(int i=0;i<10;i++){
        multiplicationTable[i]=5*(i+1);
    }
    for(int i=0;i<10;i++){
        printf("%d*%d=%d\n",num,i+1,multiplicationTable[i]);
    }

    return 0;
}

Output/-
/tmp/FjJw00S9Xv.o
Enter the value of num
5
5*1=5
5*2=10
5*3=15
5*4=20
5*5=25
5*6=30
5*7=35
5*8=40
5*9=45
5*10=50

