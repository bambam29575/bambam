# include <stdio.h>  //Write a program containing functions that counts the number of positive integers in an array.
void num(int array[],int n);
int main(){
	int array[6]={-1,-4,7,8,9,-8};
	
num(array,6);
return 0;
}
void num(int array[],int n){
	int count=0;
	for(int i=0;i<6;i++){
		if (array[i]>0){
			count++;
		}else count==0;
	}printf("%d",count);
}

output/-
3
