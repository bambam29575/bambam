// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int array[10],i;
    
    for(i=0;i<10;i++){
        printf("The value of element %d is : ",i+1);
        scanf("%d",&array[i]);
    }
    int*ptr=&array[0];
    printf("%d\n",ptr);
    printf("%u\n",ptr);
    printf("%d\n",*ptr);
    ptr+=2;
    
    printf("%d",*ptr);
    

    return 0;
}

