#include <iostream>
using namespace std;

int main() {
  int num;
  cin >> num;
  char ch;
  ch = 'A';
  
  for(int r = 1; r <= num; r++){
      
      for(int c = 1; c <= num; c++ ){
          cout << ch;
          ch++;
          
      }
    
      cout << endl;
      ch-=2;
      
      
     
    return 0;
     
     
  }
