#include <iostream>
using namespace std;

int main() {
    int n, row, space, star, num = 1;
    cin >> n;
    for(row = 1; row <= n; row++){
        for(space = 1; space <= row - 1; space++){
            cout << " ";
        }for(star = 1; star <= n - row + 1; star++){
            cout << num;
        }num++;
        cout << endl;
    }
    

    return 0;
}

output/-
6
111111
 22222
  3333
   444
    55
     6
