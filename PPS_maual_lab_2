#include<stdio.h>


int fib(int a);

 void main(){
int num,fibVal;
printf("Enter the value of num\n");
scanf("%d",&num);
fibVal=fib(num);
printf("%d is %d\n",num,fibVal);
for(int i=1;i<=num;i++){
printf("%d\n",fib(i));
}
}
int fib(int b){
int fibVal;
if(b==1||b==2){
return 1;
}else if (b==0){
return 0;
}else return fibVal=fib(b-1)+fib(b-2);

//for(int i=0;i<=b;i++){
//printf("%d",fib(i));
//}
//printf("Value of %d position in Fibonacci series is %d",b,fibVal);
}
    
