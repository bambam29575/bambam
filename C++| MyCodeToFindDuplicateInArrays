#include <iostream>
using namespace std;

int main(){
    int arr[] = {1,2,4,3,5}, arr1[] = {7,6,5,4,3,2,1}, i, j;
    int size = sizeof(arr)/sizeof(int);
    //cout << size;
    for(i = 0; i < size; i++ ){
        for(j = 0; j < 7; j++){
            if(arr[j] == arr[i]){
                cout << arr[j] << " ";
            }
        }
    }
    return 0;
}

