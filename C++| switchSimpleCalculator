#include <iostream>
using namespace std;

int main() {
    int a, b ;
    char ch;
    cin >>a >>ch >>b;
    
    switch(ch){
        case '+' : cout << a + b;
        break;
        case '-' : cout << a - b;
        break;
        case '*' : cout << a * b;
        break;
        case '/' : if (b == 0)
            cout << "Undefined";
        else 
            cout << a/b;
            break;
        case '%' : cout << a % b;
        default : cout << "Invalid operator";
    }
    
    
      
    

    return 0;
}

output /-
5
/
8
0
