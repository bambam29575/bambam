# include <stdio.h> 
//Create an array of size 3x10 containing multiplication tables of the numbers 2,7 and 9, respectively.
int main(){
int array[3][10],i,num1=2,num2=7,num3=9;

for (int i=0;i<10;i++ ){
	array[0][i]=num1*(i+1);
	array[1][i]=num2*(i+1);
	array[2][i]=num3*(i+1);
}
for (int i=0;i<10;i++ ){
	printf(" %d ",array[0][i]);
}printf("\n");
for (int i=0;i<10;i++ ){
	printf(" %d ",array[1][i]);
}printf("\n");
for (int i=0;i<10;i++ ){
	printf(" %d ",array[2][i]);
	}
return 0;
}

