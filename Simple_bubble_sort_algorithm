// Online C compiler to run C program online
#include <stdio.h>
void bubble_sort(int array[],int size);

int main() {
    int array[]={12,23,5623,123,432,566,234},i,size;
    size=sizeof(array)/sizeof(int);
    bubble_sort(array,size);
        for(i=0;i<size-1;i++){
            printf("%d ",array[i]);
     }
    

    return 0;
}
void bubble_sort(int array[],int size){
int i,j,temp;
    for(i=0;i<size-1;i++){
        for(j=0;j<size-1-i;j++){
            if(array[j]>array[j+1]){
                temp=array[j];
                array[j]=array[j+1];
                array[j+1]=temp;
            }
        }
    }
  
     
    
    
}
