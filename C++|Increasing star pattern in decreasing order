#include <iostream>
using namespace std;

int main() {
  
  int row;
  
  cin >> row;
  
  for(int i = 1; i <= row; i++){
      
    for(int j = row; j > row-i; j--){
    
    cout << "*"; //If you want to add r1->1 || r2->22 || r3->333 then replace * with i
    
      
    }
    cout << endl;
    
  }
  return 0;
}
